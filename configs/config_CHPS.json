{
    "device": "cuda:1",
    "dataset": "cars",
    "method": "CHPS",
    "geometry": "hyperspherical",
    "grad": false,
    "validation": false,
    "distortion": false,
    "lam": 0,
    "output_dim": 64,
    "batch_size": 64,
    "epochs": 200,
    "model": "resnet18",
    "temperature": 1,
    "th": 0,
    "curvature": 1,
    "clipping":1,
    "proto_lr":0,
    "num_proto":1,
    "optimizer": {
        "name": "sgd",
        "learning_rate": 1e-1,
        "weight_decay": 5e-4,
        "momentum": 0.9
    },
    "lr_scheduler": {
        "steps": [60, 120, 160],
        "entity": 0.2
    }
}
