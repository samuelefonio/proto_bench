
{
    "device": "cuda:1",
    "dataset": "aircraft",
    "method": "HPS",
    "geometry": "euclidean",
    "grad": false,
    "validation":false,
    "distortion":false,
    "lam":1.0,
    "output_dim": 64,
    "batch_size": 128,
    "epochs": 200,
    "model": "resnet18",
    "temperature": 1,
    "th": 0,
    "curvature": 1,
    "clipping":1,
    "proto_lr":0,
    "num_proto":1,
    "optimizer": {
        "name": "sgd",
        "learning_rate": 1e-2,
        "weight_decay":1e-4,
        "momentum":0.9
    },
    "lr_scheduler": {
        "steps": [100, 180], 
        "entity": 0.1
    }
}
