{
    "device": "cpu",
    "dataset": "mnist",
    "method": "MGP",
    "use_centroid": false,
    "validation": false,
    "output_dim": 8,
    "batch_size": 128,
    "epochs": 20,
    "seed": 0,
    "model": "simplecnn",
    "save_model": true,
    "eval_every": 5,
    "temperature": 1.0,
    "optimizer": {
        "name": "sgd",
        "learning_rate": 1e-1,
        "weight_decay": 1e-4,
        "momentum": 0.9
    },
    "lr_scheduler": {
        "steps": [10, 18], 
        "entity": 0.2
    },
    "logger": {
        "type": "wandb",
        "wandb_args":{
            "project": "proto_bench",
            "entity": "mlgroup"}
    }
}
