{
    "device": "cuda:1",
    "dataset": "cifar100",
    "method": "XE",
    "geometry": "euclidean",
    "grad": false,
    "validation": false,
    "distortion": false,
    "lam": 1.0,
    "output_dim": 64,
    "batch_size": 64,
    "epochs": 200,
    "model": "resnet18",
    "temperature": 1,
    "proto_lr": 0,
    "proto_wd": 0,
    "curvature": 0,
    "clipping": 0,
    "mu": 0,
    "beta": 0,
    "num_proto":1,
    "th":0,
    "optimizer": {
        "name": "sgd",
        "learning_rate": 1e-1,
        "weight_decay": 5e-4,
        "momentum": 0.9
    },
    "lr_scheduler": {
        "steps": [60, 120, 160], 
        "entity": 0.2
    }
}
